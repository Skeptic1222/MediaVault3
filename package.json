  "scripts": {
    "dev": "npx tsx server/index-production.ts",
    "build:client": "vite build",
    "build:server": "npx esbuild server/index-production.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/index-production.js",
    "start:dev": "npx tsx server/index-production.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:setup": "node scripts/setup-postgres.cjs",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "monitor:files": "powershell ./monitoring/file-watcher.ps1",
    "monitor:health": "powershell ./monitoring/health-check.ps1",
    "security:validate": "npx ts-node server/config/validator.ts",
    "security:generate": "npx ts-node server/config/validator.ts --generate",
    "security:setup": "powershell ./scripts/secure-credentials.ps1 -Action setup"
